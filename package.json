{
  "name": "@walkup/walkup-utils",
  "version": "1.0.1",
  "description": "A collection of lightweight utility functions for web development",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "npm run typecheck && rollup -c",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustinwalkup/walkup-utils.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "utils",
    "utilities",
    "web-development",
    "promise"
  ],
  "author": "Dustin Walkup <dustinwalkup@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-typescript2": "^0.34.0",
    "typescript": "^5.0.0",
    "vitest": "^0.34.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
